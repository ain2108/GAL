def int print_edge(edge e){
    print_str("(");
    print_str(source(e));
    print_str(", ");
    print_int(weight(e));
    print_str(", ");
    print_str(dest(e));
    print_str(")");
    print_endline();
    return 0;
}

def edge build_edge(string src, int w, string dst){
    edge e1;
    e1 = | src, w, dst|;
    return e1;
}

