def int print_nlist(nlist l1){
    int len;
    len = nlength(l1);
    nlist tmp;
    tmp = l1;
    int i;
    i = 0;
    print_str("->");
    while (i < len) {
          print_elist(npeek(tmp));
          print_str("::");
          tmp = nnext(tmp);
          i = i + 1;
    }
    print_endline();

    return 1;
}

def int main(){
    
    slist l1;
    l1 = ["A"::"B"::"C"];
    print_slist(l1);

    node n1;
    n1 = |"A": 2, "B", 11, "C", 2, "D"|;
    elist l2;
    l2 = n1;
    print_elist(l2);

    ilist l3;
