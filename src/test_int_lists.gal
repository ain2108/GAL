def int print_sl_len(slist lister){
    print_int(slength(lister));
    print_endline();
    return 0;
}

def int print_slist(slist l1){
    int len;
    len = slength(l1);
    slist tmp;
    tmp = l1;
    int i;
    i = 0;
    print_str("->");
    while (i < len) {
          print_str(speek(tmp));
          print_str("::");
          tmp = snext(tmp);
          i = i + 1;
    }
    print_endline();

    return 1;
}

def int print_edge(edge e){
    print_str("|");
    print_str(source(e));
    print_str(", ");
    print_int(weight(e));
    print_str(", ");
    print_str(dest(e));
    print_str("|");
    return 0;
}

def int print_elist(elist l1){
    int len;
    len = elength(l1);
    elist tmp;
    tmp = l1;
    int i;
    i = 0;
    print_str("->");
    while (i < len) {
          print_edge(epeek(tmp));
          print_str("::");
          tmp = enext(tmp);
          i = i + 1;
    }
    print_endline();

    return 1;
}

